{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vNetName": {
            "value": "ntier-vnet"
        },
        "subnetName": {
            "value": "web-subnet"
        },
        "vmSize": {
            "value": "Standard_A1_v2"
        },
        "externalLoadBalancerName": {
            "value": "external-lb"
        },
        "externalLoadBalancerBackendPoolName": {
            "value": "external-lb-backend-pool-web"
        },
        "vmPassword": {
            "value": "dummy value, provided at build time, see .github/deploy-azure-arm.yml"
        },
        "vmUsername": {
            "value": "dummy value, provided at build time, see .github/deploy-azure-arm.yml"
        },
        "acceleratedNetworking": {
            "value": false
        },
        "scaleSetName": {
            "value": "web-scale-set"
        },
        "vmCount": {
            "value": 1
        },
        "vmNamePrefix": {
            "value": "web-vm"
        },
        "vmExtensions": {
            "value": [
                {
                    "type": "Microsoft.Compute/virtualMachines/extensions",
                    "name": "[variables('startUpExtensionName')]",
                    "properties": {
                        "autoUpgradeMinorVersion": true,
                        "publisher": "Microsoft.Azure.Extensions",
                        "type": "CustomScript",
                        "typeHandlerVersion": "2.0",
                        "settings": {
                            "fileUris": [
                                "https://raw.githubusercontent.com/zmad5306/node-client-1/main/scripts/ubuntu/install.sh"
                            ]
                        },
                        "protectedSettings": {
                            "commandToExecute": "sh ./install.sh"
                        }
                    }
                }
            ]
        }
    }
}